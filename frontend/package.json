{
  "name": "boilerplate",
  "version": "0.1.0",
  "private": true,
  "description": "ui for boilerplate",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "coverage": "vitest run --coverage",
    "dev": "vite dev",
    "devBuild": "vite build --mode development",
    "fix": "eslint_d --cache --fix . && prettier --write .",
    "lint": "eslint_d --cache . && prettier --check .",
    "test": "vitest --reporter verbose --update",
    "test:ci": "vitest run --reporter verbose --update"
  },
  "dependencies": {
    "@unhead/vue": "^2.0.8",
    "axios": "^1.9.0",
    "pinia": "^3.0.2",
    "vue": "^3.5.13",
    "vue-native-websocket-vue3": "^3.1.8",
    "vue-router": "^4.5.1",
    "vuetify": "^3.8.3"
  },
  "devDependencies": {
    "@mdi/font": "^7.0.96",
    "@mdi/js": "^7.0.96",
    "@nabla/vite-plugin-eslint": "^2.0.2",
    "@pinia/testing": "^1.0.0",
    "@types/node": "^24.0.3",
    "@unhead/addons": "^2.0.14",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/eslint-plugin": "^1.1.7",
    "@vue/test-utils": "^2.2.1",
    "eslint-plugin-vue": "^10.0.0",
    "eslint-plugin-vue-scoped-css": "^2.2.0",
    "jsdom-global": "^3.0.2",
    "sass": "^1.55.0",
    "sass-loader": "^16.0",
    "typeface-roboto": "^1.1.13",
    "vite": "^7.1.0",
    "vite-plugin-checker": "^0.10.2",
    "vite-plugin-dynamic-base": "^1.0.0",
    "vite-plugin-run": "^0.6.0",
    "vite-plugin-vuetify": "^2.0.1",
    "vitest": "^3.0.2",
    "vue-eslint-parser": "^10.1.1"
  }
}
